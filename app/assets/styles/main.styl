html, 
body
	height: 100%;


.browserupgrade {
	margin: 0.2em 0;
	background: #ccc;
	color: #000;
	padding: 0.2em 0;
}

body
	padding 0
	font-family font_regular
	position relative

.overlay
	content url('../images/bgv13.png')
	position absolute
	top 0
	
	@media (max-width: 768px)
		display none

.b-page
	padding 0
	position relative
	z-index 4

	&--fixed
		padding-top 104px

.b-page__line
	padding 0
	background #fff
	
	&--main
		background url('../images/main-bg.png') no-repeat top center
		
	&--green
		background cl_gren
	
	&--blue
		background cl_blue
		
	&--white
		background #fff
		
	&--gray
		background #e8e8e8
	
	&--map
		position relative
		
	&--fixed
		position fixed
		top 0
		left 0
		right 0
		box-shadow 0 1px 5px #000
		z-index 2001
		
		.b-header
			padding-top 10px
			padding-bottom 10px
		
		.b-menu
			padding-bottom 10px

.b-header
	width 1127px
	margin 0 auto
	padding-top 30px
	padding-bottom 45px
	display flex
	justify-content space-between
	align-items center
	transition all 0.25s ease
	
	@media (max-width: 1127px)
		width 100%
		padding-left  15px
		padding-right  15px
		flex-wrap wrap
		justify-content space-around
		
	@media (max-width: 768px)
		justify-content center
		align-items center
		padding-bottom 15px

	&__phone
		font-size 36px
		font-family font_calibli
		
		@media (max-width: 768px)
			font-size 24px

.b-logo
	@media (max-width: 768px)
		width 100%

.b-menu
	display flex
	justify-content space-between
	width 1127px
	margin 0 auto
	padding-bottom 25px
	transition all 0.25s ease
	
	@media (max-width: 1127px)
		width 100%
		padding-left  15px
		padding-right  15px
		flex-wrap wrap
		justify-content space-around
		padding-bottom 0
		
	@media (max-width: 768px)
		display none
		position absolute
		background #fff
	
	&-toggle
		position absolute
		left 15px
		display none
		
		@media (max-width: 768px)
			display block
		
		&:after
			background url('../images/icon-nav.png') no-repeat top center
			width 15px
			height 15px
			content ' '
			position absolute
			top 10px
			left 0
			
	
	&__item
		font-family font_calibli
		font-size 22px
		line-height 1
		border-bottom 1px dotted #c0c0c0
		text-transform uppercase
		color #000
		transition color 0.25s ease
		
		@media (max-width: 768px)
			display block
			width 100%
			text-align center			
		
		&:hover
		&:focus
			text-decoration none
			color cl_gren
			outline 0
		
		&--active
			text-decoration none
			color cl_gren

